{
	"name": "df-svelte-starter-kit",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"devhost": "vite --host",
		"build": "vite build",
		"preview": "vite preview --host",
		"start": "node server.cjs",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint . --fix",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.27.3",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"flowbite-svelte-icons": "^0.4.4",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.64.0",
		"svelte": "^4.0.5",
		"svelte-adapter-azure-swa": "^0.15.0",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.1.5",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vitest": "^0.32.2"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.7",
		"@types/js-cookie": "^3.0.3",
		"@ubti/diligence-fabric-sdk": "^0.6.6",
		"classnames": "^2.3.2",
		"flowbite": "^1.7.0",
		"flowbite-svelte": "^0.37.5",
		"guid": "^0.0.12",
		"js-cookie": "^3.0.5",
		"svelte-forms": "^2.3.1",
		"svelte-heros-v2": "^0.7.3",
		"sweetalert2": "^11.9.0"
	}
}
